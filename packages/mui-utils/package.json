    "react-is": "^19.1.1"
  },
  "devDependencies": {
    "@mui/internal-test-utils": "workspace:^",
    "@mui/types": "workspace:^",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-is": "^19.0.0",
